{
  "name": "devhints-engine",
  "description": "Static site generator that builds devhints.io",
  "version": "0.1.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "dependencies": {},
  "devDependencies": {
    "babel-jest": "^23.0.0-alpha.0",
    "jest": "^22.4.2"
  },
  "jest": {
    "setupFiles": [
      "./packages/devhints-gatsby/tests/.jest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/packages/devhints-gatsby/.cache/",
      "<rootDir>/.cache/"
    ]
  },
  "keywords": [
    "devhints"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "private": true,
  "scripts": {
    "build": "cd packages/devhints-gatsby && yarn run build",
    "develop": "cd packages/devhints-gatsby && yarn run develop",
    "fix": "devhints-validator \"sheets/*.md\" --fix",
    "flow": "flow",
    "lint": "eslint packages/*/lib packages/*/src",
    "lint-md": "devhints-markdown-linter sheets/*",
    "test": "jest",
    "validate": "devhints-validator \"sheets/*.md\""
  },
  "workspaces": [
    "packages/*"
  ]
}
