{
  "name": "devhints-engine",
  "private": true,
  "description": "Static site generator that builds devhints.io",
  "version": "1.0.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "dependencies": {
    "gatsby": "^1.9.202",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-netlify-cms": "^1.0.12",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-source-filesystem": "1.5.20",
    "gatsby-transformer-remark": "^1.7.32",
    "isotope-layout": "^3.0.5",
    "meow": "^4.0.0",
    "react-helmet": "^5.2.0",
    "recompose": "^0.26.0",
    "rehype-react": "^3.0.2",
    "responsive-modular-scale.css": "^0.1.0",
    "sanitize.css": "^5.0.0"
  },
  "keywords": [
    "devhints"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "ci": "run-s test lint build",
    "deploy": "run-s build deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d public",
    "develop": "gatsby develop",
    "fix": "node packages/sheet-linter/bin.js \"sheets/*.md\" --fix",
    "lint": "run-s lint:js lint:md",
    "lint:js": "eslint \"src/\"",
    "lint:md": "node packages/sheet-linter/bin.js \"sheets/*.md\"",
    "test": "jest"
  },
  "devDependencies": {
    "babel-eslint": "8",
    "babel-jest": "^22.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "4",
    "eslint-config-standard": "11",
    "eslint-config-standard-react": "5",
    "eslint-plugin-flowtype": "2",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "5",
    "eslint-plugin-promise": "3",
    "eslint-plugin-react": "7",
    "eslint-plugin-standard": "3",
    "gh-pages": "^1.1.0",
    "hast-util-to-html": "^3.1.0",
    "hastscript": "^3.1.0",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "16"
  },
  "jest": {
    "setupFiles": [
      "./tests/.jest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
