{
  "name": "devhints-engine",
  "description": "Static site generator that builds devhints.io",
  "version": "0.1.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "bin": {
    "devhints-validator": "packages/devhints-validator/bin/devhints-validator"
  },
  "dependencies": {
    "array-flatten": "^2.1.1",
    "debug": "^3.1.0",
    "fastmatter": "^2.1.0",
    "fs-extra": "^7.0.0",
    "gatsby": "^1.9.236",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-styled-jsx": "^2.0.6",
    "gatsby-react-router-scroll": "^1.0.13",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.37",
    "glob": "^7.1.2",
    "group-by": "^0.0.1",
    "hint.css": "^2.5.0",
    "isotope-layout": "^3.0.5",
    "js-yaml": "^3.12.0",
    "loadjs": "^3.5.4",
    "meow": "^5.0.0",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.27.1",
    "rehype-react": "^3.0.3",
    "remark": "^9.0.0",
    "remark-cli": "^5.0.0",
    "remark-frontmatter": "^1.2.0",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "responsive-modular-scale.css": "^0.1.0",
    "sanitize.css": "^6.0.0",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-postcss": "^0.1.3"
  },
  "devDependencies": {
    "@andrew-codes/gatsby-plugin-elasticlunr-search": "^1.0.4",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.77.0",
    "gh-pages": "^1.1.0",
    "hast-util-to-html": "^4.0.1",
    "hastscript": "^4.0.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.4.1",
    "standard": "^11.0.1"
  },
  "jest": {
    "setupFiles": [
      "./tests/.jest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ],
    "testURL": "http://localhost",
    "moduleNameMapper": {
      ".*raw-loader.*": "<rootDir>/src/lib/noop-string.js"
    }
  },
  "keywords": [
    "devhints"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths && (cd public && bash ../packages/devhints-scripts/postbuild.sh)",
    "develop": "rm -rf .cache; env DEBUG='app:*' gatsby develop --port ${PORT:-19336}",
    "fix": "devhints-validator \"sheets/*.md\" --fix",
    "flow": "flow",
    "lint": "eslint packages/*/lib src",
    "prepush": "yarn run prettier:check",
    "prettier:check": "prettier-eslint --list-different \"sheets/**/*.md\" \"{src,packages}/**/*.{js,css}\" \"./*.js\"",
    "prettier:fix": "prettier-eslint --write \"sheets/**/*.md\" \"{src,packages}/**/*.{js,css}\" \"./*.js\"",
    "start": "yarn run develop",
    "test": "jest",
    "validate": "devhints-validator \"sheets/*.md\""
  },
  "workspaces": [
    "packages/*"
  ]
}
