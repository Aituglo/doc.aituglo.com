{
  "name": "devhints-engine",
  "private": true,
  "description": "Static site generator that builds devhints.io",
  "version": "1.0.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "dependencies": {},
  "keywords": [
    "devhints"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "deploy": "run-s build deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d public",
    "develop": "gatsby develop",
    "fix": "devhints-validator \"sheets/*.md\" --fix",
    "flow": "flow",
    "lint": "eslint src/ packages/",
    "lint-md": "devhints-markdown-linter sheets/*",
    "validate": "devhints-validator \"sheets/*.md\"",
    "test": "jest"
  },
  "devDependencies": {
    "babel-jest": "^23.0.0-alpha.0",
    "jest": "^22.4.2"
  },
  "jest": {
    "setupFiles": [
      "./packages/devhints-gatsby/tests/.jest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
