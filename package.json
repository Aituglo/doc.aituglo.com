{
  "name": "devhints-engine",
  "description": "Static site generator that builds devhints.io",
  "version": "0.1.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "bin": {
    "devhints-validator": "packages/devhints-validator/bin/devhints-validator"
  },
  "dependencies": {
    "array-flatten": "^2.1.1",
    "debug": "^3.1.0",
    "fastmatter": "^2.0.1",
    "fs-extra": "^5.0.0",
    "gatsby": "^1.9.236",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-netlify-cms": "^1.0.12",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-styled-jsx": "^2.0.6",
    "gatsby-react-router-scroll": "^1.0.13",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.37",
    "glob": "^7.1.2",
    "isotope-layout": "^3.0.5",
    "js-yaml": "^3.11.0",
    "meow": "^4.0.0",
    "prop-types": "^15.6.1",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-helmet": "^5.2.0",
    "recompose": "^0.26.0",
    "rehype-react": "^3.0.2",
    "remark": "^9.0.0",
    "remark-cli": "^5.0.0",
    "remark-frontmatter": "^1.2.0",
    "remark-preset-lint-markdown-style-guide": "^2.1.1",
    "responsive-modular-scale.css": "^0.1.0",
    "sanitize.css": "^5.0.0",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-postcss": "^0.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.68.0",
    "gh-pages": "^1.1.0",
    "hast-util-to-html": "^3.1.0",
    "hastscript": "^3.1.0",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "setupFiles": [
      "./tests/.jest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ]
  },
  "keywords": [
    "devhints"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "ci:lint": "run-p flow lint validate format:list",
    "ci:test": "run-p test build",
    "develop": "rm -rf .cache; env DEBUG='app:*' gatsby develop",
    "fix": "devhints-validator \"sheets/*.md\" --fix",
    "format": "prettier-eslint --write \"sheets/**/*.md\" \"{src,packages}/**/*.{js,css}\"",
    "format:list": "prettier-eslint --list-different \"sheets/**/*.md\" \"{src,packages}/**/*.{js,css}\"",
    "flow": "flow",
    "lint": "eslint packages/*/lib src",
    "test": "jest",
    "validate": "devhints-validator \"sheets/*.md\""
  },
  "workspaces": [
    "packages/*"
  ]
}
